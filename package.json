{
  "name": "react-time-duration-input",
  "version": "1.0.0",
  "description": "React component for input time durations",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage",
    "lint": "eslint ./src/**/*.js",
    "prepublishOnly": "babel ./src --ignore src/**/*.test.js --out-dir ./dist -s inline",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BillRobitskeJr/react-time-duration-input.git"
  },
  "keywords": [
    "react",
    "input",
    "time",
    "duration",
    "component"
  ],
  "author": "Bill Robitske, Jr. <bill.robitske.jr@gmail.com> (https://github.com/BillRobitskeJr)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BillRobitskeJr/react-time-duration-input/issues"
  },
  "homepage": "https://github.com/BillRobitskeJr/react-time-duration-input#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^8.0.4",
    "babel-eslint": "^10.0.2",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.5.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "semantic-release": "^19.0.3"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/*.test.js",
      "!src/index.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
